#include<bits/stdc++.h>
using namespace std;

//Polymorphism : Same Function doing many jobs
//Compile time Polymorphism
//Function Overloading

class A{
public:
	void add(int x,int y)
	{
		cout<<x+y<<endl;
	}
	void add(int x,int y,int z)
	{
		cout<<x+y+z<<endl;
	}
};

int main()
{
	A a;
	a.add(1,2);
	a.add(1,2,3);
	return 0;
}
